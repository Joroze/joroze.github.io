{
    "name": "joroze.github.io",
    "description": "Jordan's personal resume/portfolio website in React",
    "version": "1.0.0",
    "homepage": "https://joroze.github.io/",
    "bugs": "https://github.com/Joroze/joroze.github.io/issues",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Joroze/joroze.github.io.git"
    },
    "author": {
        "name": "Jordan Rosenberg",
        "email": "roseje57@oneonta.edu",
        "url": "http://joroze.com"
    },
    "private": true,
    "dependencies": {
        "axios": "0.18.0",
        "lodash": "4.17.4",
        "moment": "2.18.1",
        "qs": "^6.5.1",
        "ramda": "0.25.0",
        "react": "16.0.0",
        "react-dom": "16.0.0",
        "react-redux": "5.0.5",
        "react-router": "4.1.1",
        "react-router-dom": "4.1.1",
        "react-scroll": "1.5.4",
        "redux": "4.0.0",
        "redux-form": "7.0.4",
        "redux-observable": "0.18.0",
        "redux-thunk": "2.2.0",
        "rxjs": "6.0.0",
        "rxjs-compat": "6",
        "semantic-ui-css": "2.2.11",
        "semantic-ui-react": "0.79.1",
        "typed.js": "2.0.5"
    },
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "NODE_PATH=src/ npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "gh-pages": "1.0.0",
        "node-sass-chokidar": "1.2.2",
        "npm-run-all": "4.0.2",
        "react-scripts": "1.1.4",
        "redux-devtools-extension": "2.13.2"
    }
}
