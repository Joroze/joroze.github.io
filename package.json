    "homepage": "https://joroze.github.io/",
    "bugs": "https://github.com/Joroze/joroze.github.io/issues",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Joroze/joroze.github.io.git"
    },
    "author": {
        "name": "Jordan Rosenberg",
        "email": "roseje57@oneonta.edu",
        "url": "http://joroze.com"
    },
    "private": true,
    "dependencies": {
        "axios": "^0.16.2",
        "gh-pages": "^1.0.0",
        "lodash": "^4.17.4",
        "moment": "^2.18.1",
        "node-sass-chokidar": "^0.0.3",
        "npm-run-all": "^4.0.2",
        "query-string": "^5.0.0",
        "ramda": "^0.24.1",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "react-scripts": "1.0.13",
        "react-scroll": "^1.5.4",
        "redux": "^3.7.2",
        "redux-observable": "^0.16.0",
        "redux-thunk": "^2.2.0",
        "rxjs": "^5.4.3",
        "semantic-ui-css": "^2.2.11",
        "semantic-ui-react": "^0.73.0",
        "typed.js": "^2.0.5"
    },
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "NODE_PATH=src/ npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "redux-devtools-extension": "^2.13.2"
    }
}
